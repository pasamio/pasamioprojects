	<author>Joomla! Project/Sam Moffatt</author>
	<creationDate>March 2006</creationDate>
	<copyright>(C) 2005 Open Source Matters. All rights reserved.</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<authorEmail>sam.moffatt@joomla.org</authorEmail>
	<authorUrl>sammoffatt.com.au</authorUrl>
	<version>1.0.0</version>
	<description>This module shows the advanced admin menu.</description>
	<files>
		<filename module="mod_advmenu">mod_advmenu.php</filename>
		<filename>menu.php</filename>
		<filename>menu.css</filename>
		<filename>init.js</filename>
		<filename>index.html</filename>
		<filename>helper.php</filename>
	</files>
	<params>
	</params>
	<params group="advanced">
		<param name="cache" type="list" default="1" label="Caching" description="Select whether to cache the content of this module">
